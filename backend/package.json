{
  "name": "advisory-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Advisory Tracker application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "knex migrate:latest --knexfile src/config/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile src/config/knexfile.js",
    "migrate:make": "knex migrate:make --knexfile src/config/knexfile.js",
    "seed": "knex seed:run --knexfile src/config/knexfile.js"
  },
  "keywords": [
    "advisory",
    "tracker",
    "backend",
    "api",
    "express",
    "knex",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^5.1.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.1",
    "objection": "^3.1.5",
    "pg": "^8.11.0",
    "puppeteer": "^24.16.0",
    "xlsx": "^0.18.5"
  },
